{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFiles": [
    "./test-environment.ts",
    "./string.extensions.ts"
  ],
  "moduleNameMapper": {
    "@root/(.*)$": [
      "<rootDir>/$1"
    ],
    "@src/(.*)$": [
      "<rootDir>/src/$1"
    ],
    "@auth/(.*)$": [
      "<rootDir>/src/auth/$1"
    ],
    "@common/(.*)$": [
      "<rootDir>/src/common/$1"
    ],
    "@files/(.*)$": [
      "<rootDir>/src/files/$1"
    ],
    "@followers/(.*)$": [
      "<rootDir>/src/followers/$1"
    ],
    "@logger/(.*)$": [
      "<rootDir>/src/logger/$1"
    ],
    "@posts/(.*)$": [
      "<rootDir>/src/posts/$1"
    ],
    "@profiles/(.*)$": [
      "<rootDir>/src/profiles/$1"
    ],
    "@roles/(.*)$": [
      "<rootDir>/src/roles/$1"
    ],
    "@security/(.*)$": [
      "<rootDir>/src/security/$1"
    ],
    "@users/(.*)$": [
      "<rootDir>/src/users/$1"
    ],
    "@test/(.*)$": [
      "<rootDir>/test/$1"
    ],
    "@assets/(.*)$": [
      "<rootDir>/assets/$1"
    ]
  }
}